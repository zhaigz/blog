    <script th:inline="javascript">
        // 手机屏导航栏隐藏
        $('.menu.toggle').click(function () {
            $('.m-item').toggleClass('m-mobile-hide');
        });
        //点击赞赏弹出二维码 
        $('#payButton').popup({
            popup: $('.payQR.popup'),
            on: 'click',
            position: 'bottom center'
        })
        // 右下角目录导航
        tocbot.init({
            // Where to render the table of contents.生成的目录放在哪
            tocSelector: '.js-toc',
            // Where to grab the headings to build the table of contents. 那些内容生成目录
            contentSelector: '.js-toc-content',
            // Which headings to grab inside of the contentSelector element.
            headingSelector: 'h1, h2, h3',
            // For headings inside relative or absolute positioned containers within content.
            hasInnerContainers: true,
        });
        // 点击右下角导航条弹出目录
        $('.toc.button').popup({
            popup: $('.toc-container.popup'),
            on: 'click',
            position: 'left center'
        })
        // 弹出二维码 需要在要点击的地方方式样式wechat,弹出位置设置样式wechat-qr
        $('.wechat').popup({
            popup: $('.wechat-qr.popup'),
            // 默认
            on: 'hover',
            position: 'left center'
        })

        // 生成二维码 扫描二维码后手机查看 qrcode要放置二维码的id
        const serurl = /*[[#{blog.serurl}]]*/"127.0.0.1:8080";
        //需要在script中加上th:inline="javascript"
        const url = /*[[@{/blog/{id}(id=${blog.id})}]]*/"";
        const qrcode = new QRCode("qrcode", {
            text: serurl + url,
            width: 110,
            height: 110,
            colorDark: "#000000",
            colorLight: "#ffffff",
            correctLevel: QRCode.CorrectLevel.H
        });
        // 平滑滚动到顶部
        $('#toTop-button').click(function () {
            // 第一个参数回到的位置，第二个使用时间
            $(window).scrollTo(0, 500);
        })

        // 滚动侦测
        var waypoint = new Waypoint({
            // 滚动的东西的位置 目标点 这里给到了大的div 中间内容部分
            element: document.getElementById('waypoint'),
            handler: function (direction) {
                if(direction == 'down'){
                    $('#toolbar').show(100);
                }else{
                    $('#toolbar').hide(500);
                }
                // up down
                // console.log('Scrolled to waypoint!'+direction)
            }
        })
        //评论表单验证 提交时利用ajax
        $('.ui.form').form({
            fields: {
                content: {
                    // identifier和表单name值保持一致
                    identifier: 'content',
                    rules: [{
                        type: 'empty',
                        prompt: '请输入评论内容'
                    }]
                },
                nickname: {
                    // identifier和表单name值保持一致
                    identifier: 'nickname',
                    rules: [{
                        type: 'empty',
                        prompt: '请输入昵称'
                    }]
                },
                email: {
                    // identifier和表单name值保持一致
                    identifier: 'email',
                    rules: [{
                        type: 'email',
                        prompt: '请填写正确的邮箱地址'
                    }]
                }
            }
        })

        //点击发送jquery请求
        $('#commentpost-btn').click(function (){
            var boo = $('.ui.form').form('validate form');
            if(boo) {
                console.log("校验成功");
                //发送ajax请求到后台
                postData();
            }else {
                console.log("校验失败");
            }
        })

        //采用Jquery的load请求
        function postData(){
            $('#comment-container').load(/*[[@{/comments}]]*/"",{
                "parentComment.id" : $("[name='parentComment.id']").val(),
                "blog.id" : $("[name='blog.id']").val(),
                "nickname" : $("[name='nickname']").val(),
                "email" : $("[name='email']").val(),
                "content" : $("[name='content']").val()
            },function (responseTxt,statusTxt,xhr){//    回调函数callback
                //提交完后滚动到评论顶部
                $(window).scrollTo($('#comment-container'),500);
                //提交后清空评论的内容区域,并且将父评论重新置为-1
                clearContent();
            })
        }

        //提交后清空评论
        function clearContent(){
            $("[name='content']").val('');
            $("[name='parentComment.id']").val(-1);
            $("[name='content']").attr("placeholder","请输入评论信息...");
        }

        //回复
        function reply(obj){
            //使用data不再需要指定为data-commentid,data方法好像不支持大写字母 拿不到值 所以没有使用驼峰命名 focus()光标定位
            var commentId=$(obj).data('commentid');
            var commentNickname = $(obj).data('commentnickname');
            $("[name='content']").attr("placeholder","@"+commentNickname).focus();
            $("[name='parentComment.id']").val(commentId);
            $(window).scrollTo($('#comment-form'),500);
        }
    </script>
</body>

</html>"
